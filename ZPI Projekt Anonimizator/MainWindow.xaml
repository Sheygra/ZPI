        </CheckBox>
        <CheckBox x:Name="secondAlgorythm" Margin="120,120,1278,864">
            second alforythm
        </CheckBox>
        <TextBox Name="testTextBox1" Margin="5,505,760,143">
            TestBox for XML testing
        </TextBox>
        <TextBox Name="testTextBox2" Margin="0,192,765,500">
            TestBox for word testing
        </TextBox>
        <TextBox Name="testTextBox3" Margin="740,192,0,500" TextChanged="testTextBox3_TextChanged">
